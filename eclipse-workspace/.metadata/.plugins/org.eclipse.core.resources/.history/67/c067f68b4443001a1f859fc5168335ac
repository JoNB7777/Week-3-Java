package com.qa.garage;

import java.util.ArrayList;

public class Garage {

	public static void main(String[] args) {
		ArrayList<Vehicle> vehicles = new ArrayList<Vehicle>();
		Car car = new Car();
		vehicles.add(car);
		car.move();
		Ship ship = new Ship();
		vehicles.add(ship);
		ship.move();
		Train train = new Train();
		vehicles.add(ship);
		train.move();
		car.crash();
		System.out.println(train.travelledDistance);

	}

}
